<zk>
	<label>Focus to target</label>
	<div viewModel="@('org.zkoss.zktest.test2.F95_ZK_4624FocusVM')">
		<div>
			<button onClick="@('doFocus')" label="focus the textbox"/>
			<button id="fIntoList" onClick="@('doFocusInList')" label="focus the textbox in a List"/>
			<button onClick="@('doFocusInner')" label="focus the textbox inner"/>
			<textbox id="tb" value="@(text)" />
		</div>
		<div id="list">
			<forEach items="@(list)">
				<textbox value="@(each)"/>
			</forEach>
			<window id="win2">
				<textbox id="tb_inner" />
			</window>
			<window id="win3">
				<textbox id="tb_inner" />
			</window>
		</div>
	</div>
</zk>
