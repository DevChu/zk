<zk>
    <label>
        HTTP 500 java.lang.IllegalArgumentException: 'step' &lt;= 0 [-2]
    </label>
    <hlayout id="multiplicationTable">
        <forEach begin="10" end="1" step="-2" var="x">
            <vlayout>
                <forEach begin="1" end="10" var="y">
                    <label value="${x} x ${y} = ${x * y}"/>
                </forEach>
            </vlayout>
        </forEach>
    </hlayout>
</zk>