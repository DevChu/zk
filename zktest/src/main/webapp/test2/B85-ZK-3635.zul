<zk>
	<style>
		input:focus, input[type="checkbox"]:focus {
			background: #ffffcf none;
			outline: dotted thin;
		}
	</style>
	<label multiline="true">
		1. Open bandbox
		2. Focus in first textbox
		3. Press Tab twice
		4. Bandbox should not close and the second textbox should receive focus.
	</label>
	<separator/>
	<bandbox>
		<bandpopup>
			<textbox/>
			<checkbox/>
			<textbox/>
		</bandpopup>
	</bandbox>
</zk>
