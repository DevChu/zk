<zk>
	<label multiline="true">
		Resize the header (drap and drop), and you should see head, body and foot align.
	</label>
	<window title="listbox demo" border="normal">
        <listbox id="box">
            <listhead sizable="true">
                <listheader label="name" sort="auto"/>
                <listheader label="gender" sort="auto"/>
            </listhead>
            <listitem>
                <listcell label="Mary"/>
                <listcell label="FEMALE"/>
            </listitem>
            <listitem>
                <listcell label="John"/>
                <listcell label="MALE"/>
            </listitem>
            <listitem>
                <listcell label="Jane"/>
                <listcell label="FEMALE"/>
            </listitem>
            <listitem>
                <listcell label="Henry"/>
                <listcell label="MALE"/>
            </listitem>
            <listfoot>
                <listfooter><label value="This is footer1"/></listfooter>
                <listfooter><label value="This is footer2"/></listfooter>
            </listfoot>
        </listbox>        
</window>
</zk>