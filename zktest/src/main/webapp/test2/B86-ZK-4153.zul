<zk>
    <hlayout>
        <custom-attributes pageSelf="${desktop.getRequestPath()}"/>
        <a href="${pageSelf}?zktheme=breeze" label="Breeze"/>
        <a href="${pageSelf}?zktheme=sapphire" label="Sapphire"/>
        <a href="${pageSelf}?zktheme=silvertail" label="Silvertail"/>
        <a href="${pageSelf}?zktheme=atlantic" label="Atlantic"/>
        <a href="${pageSelf}?zktheme=default" label="Default"/>
    </hlayout>
    <label multiline="true">
        (Only on IE11)
        1. Use F12 > UI Responsiveness to do profiling.
        2. Hover on the input fields back and forth.
        3. Check the CPU utilization and Layout times, shouldn't like the result below.
    </label>
    <html><img src="data:image/png;base64,"/></html>
    <hlayout>
        <forEach begin="1" end="5">
            <vlayout xmlns:n="native">
                <forEach begin="1" end="100">
                    <n:input type="text"/>
                </forEach>
            </vlayout>
        </forEach>
    </hlayout>
    <separator/>
    <hlayout>
        <combobox/>
        <bandbox/>
        <datebox/>
        <timebox/>
        <spinner/>
        <doublespinner/>
        <timepicker/>
    </hlayout>
    <hlayout>
        <combobox constraint="no empty"/>
        <bandbox disabled="true"/>
        <datebox inplace="true"/>
        <timebox readonly="true"/>
        <spinner buttonVisible="false"/>
        <timepicker disabled="true"/>
    </hlayout>
</zk>
