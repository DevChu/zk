<zk>
	<label>drag/drop slider buttons, then you can see values in zk log.</label>
	<multislider>
		<sliderbuttons id="limits" startValue="10" endValue="90"
					   onRangeValueChange='Clients.log(event.toString() + event.startValue + "~" + event.endValue)'/>
		<sliderbuttons id="ideal" startValue="50" endValue="70"
					   onRangeValueChange='Clients.log(event.toString() + event.startValue + "~" + event.endValue)'/>
	</multislider>
</zk>