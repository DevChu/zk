<zk xmlns:w="client">
    <vlayout>
        <label multiline="true">
            1. input integer "4" in the first spinner
            2. should see "custom error" in the popup errorbox
            3. input integer "10" in the first spinner
            4. should see "max is 9" in the popup errorbox
            5. input integer "4" in the second spinner
            6. should see "Out of range: >= 5" in the popup errorbox

        </label>
        <spinner constraint="min 5:custom error, max 9:max is 9"/>
        <spinner constraint="min 5"/>
    </vlayout>
</zk>