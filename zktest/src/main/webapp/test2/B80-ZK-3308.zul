<zk>
    <label multiline="true">
        1. click the button "toggle open close for grid", the groups of the grid should be closed, and the group icons should be changed to ">"
        2. click the same button again, the groups of the grid should be opened, and the group icons should be changed to "v"
        3. click the button "toggle open close for listbox", the groups of the listbox should be closed, and the group icons should be changed to ">"
        4. click the same button again, the groups of the listbox should be opened, and the group icons should be changed to "v"

    </label>
    <window apply="org.zkoss.zktest.test2.B80_ZK_3308composer">
        <grid id="grid"/>
        <button label="toggle open close for grid" id="btn1"/>
        <listbox id="lb"/>
        <button label="toggle open close fro listbox" id="btn2"/>
    </window>
</zk>