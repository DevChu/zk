<zk>
    <label multiline="true">
        1. Hover to the following orders
        - "menu 1" -> "menuitem 1" -> "menu 3"
        - "menu 1" -> "menu 2" -> "menuitem 4" -> "menuitem 5" -> "menuitem 7"
        2. The menu shouldn't be closed suddenly.
    </label>
    <menubar autodrop="true">
        <menu label="menu 1" id="menu1">
            <menupopup id="menu1popup">
                <menuitem label="menuitem 1" id="menuitem1"/>
                <menuitem label="menuitem 2"/>
                <menuitem label="menuitem 3"/>
                <menu label="menu 2" id="menu2">
                    <menupopup>
                        <menuitem label="menuitem 4" id="menuitem4"/>
                        <menuitem label="menuitem 5" id="menuitem5"/>
                        <menuitem label="menuitem 6"/>
                    </menupopup>
                </menu>
                <menuitem label="menuitem 7" id="menuitem7"/>
                <menuitem label="menuitem 8"/>
            </menupopup>
        </menu>
        <menu label="menu 3" id="menu3">
            <menupopup id="menu3popup">
                <menuitem label="menuitem 9"/>
                <menuitem label="menuitem 10"/>
                <menuitem label="menuitem 11"/>
            </menupopup>
        </menu>
    </menubar>
</zk>