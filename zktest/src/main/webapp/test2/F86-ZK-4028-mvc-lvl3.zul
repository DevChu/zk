<zk>
    <div style="border: 1px solid blue">
        <label value="${'Level ' += level3.level}"/>
        <label value="${level3.currentKey}"/>
        <label value="${data}" />
        <label value="${context}" />

        <apply level4="${level3.child}" dynamicValue="true" />

        <vlayout>
            <forEach items="${level3.child.items}">
                <a label="${each.x}" onClick="level3.child.navigateTo(self.label)"/>
            </forEach>
        </vlayout>
    </div>
</zk>
