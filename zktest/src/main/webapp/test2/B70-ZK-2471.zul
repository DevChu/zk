<zk>
	<label multiline="true">
		Fileupload not working in IE 10 or below
		mouse hover effects on upload button should work properly
		tooltip and tooltiptext on upload button should work properly
	</label>
	<hlayout>
    	<button autodisable="self" upload="true,multiple=false,native" tooltip="any" 
    			onUpload="Clients.showNotification(event.getMedia().getName())" label="button with tooltip"/>
    			
    	<button autodisable="self" upload="true,multiple=false,native" tooltiptext="tooltiptext seen" 
    			onUpload="Clients.showNotification(event.getMedia().getName())" label="button with tooltiptext"/>
	</hlayout>
	<div>
	    <toolbar>
	    	<toolbarbutton upload="true" tooltip="any" onUpload="Clients.showNotification(event.getMedia().getName())" label="toolbar btn with tooltip"/>
	    </toolbar>
	    <toolbar>
	    	<toolbarbutton upload="true" tooltiptext="tooltiptext seen" onUpload="Clients.showNotification(event.getMedia().getName())" label="toolbar btn with tooltiptext"/>
	    </toolbar>
	</div>

    <popup id="any" width="300px">
	    <vbox>
	        ZK simply rich.
	        <toolbarbutton label="ZK your killer Web application now!"
	            href="http://www.zkoss.org" />
	    </vbox>
	</popup>
</zk>