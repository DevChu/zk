<zk>
	<label multiline="true">
		the treecells' align should be applied as same as the treecol elements.
		
	</label>
	<tree width="400px">
		<treecols>
			<treecol label="left"/>
			<treecol align="center" label="center"/>
			<treecol align="right" label="right"/>
		</treecols>
		<treechildren>
			<treeitem>
				<treerow>
					<treecell>
						<label value="left"/>
					</treecell>
					<treecell>
						<label value="center"/>
					</treecell>
					<treecell>
						<label value="right"/>
					</treecell>
				</treerow>
				<treechildren>
					<treeitem>
						<treerow>
							<treecell>
								<label value="left"/>
							</treecell>
							<treecell>
								<label value="center"/>
							</treecell>
							<treecell>
								<label value="right"/>
							</treecell>
						</treerow>
					</treeitem>
				</treechildren>
			</treeitem>
		</treechildren>
	</tree>
</zk>