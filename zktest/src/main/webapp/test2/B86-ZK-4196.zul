<zk>
	<label multiline="true">
		1. Click requestCamera
		2. Click stopStreaming
		3. Camera should stop streaming (streaming icon on browser should be removed)
		4. Click requestCamera
		5. Click detach
		6. Camera should stop streaming (streaming icon on browser should be removed)
	</label>
	<camera id="camera" width="300px"/>
	<button label="requestCamera" onClick="camera.requestCamera()"/>
	<button label="stopStreaming" onClick="camera.stopStreaming()"/>
	<button label="detach" onClick="camera.detach()"/>
</zk>