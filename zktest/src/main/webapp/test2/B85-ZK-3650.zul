<zk>
    <label multiline="true">
        1. click arrow up of the spinner, the value of the textbox should also change to 1
        2. click out side of the spinner
        3. click arrow up of the spinner again, the value of the textbox should change to 2

    </label>
    <hbox xmlns:w="client">
        onChanging
        <spinner w:onChanging="this.nextSibling.setValue(event.value)" />
        <textbox />
    </hbox>
</zk>
