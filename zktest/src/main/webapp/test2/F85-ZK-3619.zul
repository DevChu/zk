<zk>
	<label multiline="true">
		1. Click "NO PAST" button.
		2. Should not be able to select past.
		3. Click "NO FUTURE" button.
		4. Should not be able to select future.
	</label>
	<calendar id="calendar" width="600px"/>
	<button label="NO PAST" onClick='calendar.setConstraint("no past")'/>
	<button label="NO FUTURE" onClick='calendar.setConstraint("no future")'/>
</zk>