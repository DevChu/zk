<zk>
	<div>Click on "Add Item". There should be no javascript error.</div>
	<button label="Add Item">
		<attribute name="onClick"><![CDATA[
			Listitem item = new Listitem("item");
			lb.appendChild(item);
			item.setSelected(true);
		]]></attribute>
	</button>
	<bandbox>
		<bandpopup>
			<listbox id="lb" />
		</bandpopup>
	</bandbox>
</zk>
