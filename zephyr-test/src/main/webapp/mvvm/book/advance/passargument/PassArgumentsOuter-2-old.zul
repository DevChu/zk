<zk>
	<label multiline="true">
		load the page, shall see "myArgument" and "myArgument" bellow.
		click the cmd button, "myArgument" labels shall not change.
	</label>
	<div id="outerVM" viewModel="@id('vm') @init('org.zkoss.zephyr.test.mvvm.book.advance.PassArgumentsOuterVM')">
		<include type="@load(vm.myArgument)" abc="@load(vm.myArgument)" src='@load(c:cat(vm.myArgument, ".zul"))'/>
		<separator />
		<label value="@load(vm.myArgument)"/>
		<button label="cmd" onClick="@command('cmd')"/>
	</div>
</zk>