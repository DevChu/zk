<window viewModel="@id('vm') @init('org.zkoss.clientbind.test.issue.B00762Combobox2')">
	<vlayout>
	<label id="msg" value="@bind(vm.message1)"/>
	</vlayout>
	<combobox id="outerbox" width="300px" model="@bind(vm.items)">
		<template name="model" var="item">
			<comboitem label="@bind(item.name)"/>
		</template>
	</combobox>
	<hbox>
		<label id="min" value="@load(vm.min)" />
	</hbox>
	<hbox>
		<button id="clean" label="clean" onClick="@command('clean')" />
		<button id="select" label="select 1" onClick="@command('select')" />
		<button id="reload" label="reload" onClick="@command('reload')" />
		<button id="showselect" label="show selection" onClick="@command('show')" />
		
		<button label="Dump" onClick="binder.getTracker().dump()" />
	</hbox>
</window>